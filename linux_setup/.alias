alias a alias

a a			alias
a .			suspend
a h			history
a cd		'cd \!*;set prompt="$cwd ! > ";ls -Fap'
a rm		rm -if

#-----------------------------------------------------------------------------------------
#		property
#-----------------------------------------------------------------------------------------

set			k_tb = 
a ls		'ls -l --color'
a l			'ls -asF'
a ll		'ls -lasF'
a vi		'/usr/bin/vim -u ~/.vimrc'
a v			'/usr/bin/vim -u ~/.vimrc'
a ctags		'ctags --fields=+l'
a dirs		'dirs -v'

a m3d		'cd ~/project/tool/m3d/'
a srvlin	'ssh -X ecelinsrv1.ece.gatech.edu'
a srvleg	'ssh -X ecelegacylin.ece.gatech.edu'
a srv10		'ssh -X gtcad-srv10.ece.gatech.edu'
a srv9		'ssh -X gtcad-srv9.ece.gatech.edu'
a srv8		'ssh -X gtcad-srv8.ece.gatech.edu'
a srv7		'ssh -X gtcad-srv7.ece.gatech.edu'
a srv6		'ssh -X gtcad-srv6.ece.gatech.edu'
a srv5		'ssh -X gtcad-srv5.ece.gatech.edu'
a srv4		'ssh -X gtcad-srv4.ece.gatech.edu'
a srv3		'ssh -X gtcad-srv3.ece.gatech.edu'
a srv2		'ssh -X gtcad-srv2.ece.gatech.edu'
a srv1		'ssh -X gtcad-srv1.ece.gatech.edu'

a ps		'ps -f'
a kill		'kill -9'
a lic		'lmstat -f Innovus_Impl_System'
a du		'du -sh *'
a df		'df -h .'

##########################################################################################
#                           END
